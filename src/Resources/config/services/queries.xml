<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service
            id="sylius_refund.doctrine.orm.query.count_order_item_unit_refunds_belonging_to_order"
            class="Sylius\RefundPlugin\Doctrine\ORM\CountOrderItemUnitRefundsBelongingToOrderQuery"
        >
            <argument type="service" id="sylius.repository.order_item_unit" />
        </service>

        <service
            id="sylius_refund.doctrine.orm.query.count_shipment_refunds_belonging_to_order"
            class="Sylius\RefundPlugin\Doctrine\ORM\CountShipmentRefundsBelongingToOrderQuery"
        >
            <argument type="service" id="sylius.repository.adjustment" />
        </service>
    </services>
</container>
